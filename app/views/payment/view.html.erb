    <!-- contact -->
  
   
  <div class="limiter">
  <div class="container-table100">
  <div class="wrap-table100" style="margin-left:-100px">
  <div class="table100 ver1 m-b-110">
          

          <div class="table100-body js-pscroll">
            <table>
               <thead>
                <tr class="row100 head">
                  <th class="cell100 column1">Customer Name</th>
                  <th class="cell100 column2">Date</th>
                  <th class="cell100 column3">Time</th>
                  <th class="cell100 column4">Type</th>
                  <th class="cell100 column5">Total Price</th>
                  <th class="cell100 column6">Payment Credentials</th>
                  <th  rowspan="2" scope="rowgroup" class="cell100 column7">Items Sold</th>
                </tr>
              </thead>
              <tbody>
              
              <% @transactions.each do |t| %>
              
              <tr class="row100 body">
                  <%name=""%>
                  <% cu = customer(t.data[:customerId])%>
                  <%if !cu.blank? %>
                  <% cu.first(1).each do |c| %>
                  <%name=c.data[:displayName]%>
                  <% puts c %>
                  <%end%>
                  <%end%>
                  <td class="cell100 column1"><%= t.data[:customerId]%></td>
                  <td  class="cell100 column2"><%= t.data[:date]%></td>
                  <td   class="cell100 column3"><%= t.data[:time]%></td>
                  <td  class="cell100 column4"><%= t.data[:type]%></td>
                  <td class="cell100 column5"><%= t.data[:total]%></td>
                  <td  class="cell100 column6"> </td>
                  <td  class="cell100 column7">
                      <% sale = salelist(t.document_id)%>
                      
                      <% sale.each do |s|%>
                        <%if s %>
                        <% puts s.data%>
                        <% p = product(s.data[:itemId])%>
                         <p ><%= p.data[:name]%>     <%= s.data[:quantity]%></p>  
                     <%end%>
                     <%end%>
                   </td>


              </tr>
              
              <%end%>
              
              </tbody>
            </table>
          </div>
        </div>
        </div>
    </div>
  </div>



